{
  "env": {
    "node": true,
    "commonjs": true,
    "es2020": true
  },
  "extends": ["plugin:prettier/recommended", "airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 11
  },
  "rules": {
    "prettier/prettier": "error",
    "comma-dangle": "off",
    "camelcase": "off",
    "object-curly-newline":"off"
  },
  "plugins": ["prettier"]
}
